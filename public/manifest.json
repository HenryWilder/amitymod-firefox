{
    "manifest_version": 2,
    "name": "Amity Mod",
    "version": "0.1",

    "description": "I thought it'd be neat",
    "homepage_url": "https://github.com/HenryWilder/amitymod-firefox",
    "icons": {
        "512": "icons/goop-512.png"
    },

    "permissions": ["activeTab"],

    "browser_action": {
        "default_icon": "icons/goop-512.png",
        "default_title": "Amity Mod",
        "default_popup": "popup/index.html"
    },

    "web_accessible_resources": [ ]
}
